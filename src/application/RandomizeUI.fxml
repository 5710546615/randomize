<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane hgap="10.0" prefHeight="400.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RandomizeController">
   <children>
      <Label alignment="CENTER" prefHeight="17.0" prefWidth="552.0" text="Randomize" />
      <Button mnemonicParsing="false" onAction="#changeSceneToNumber" prefWidth="150.0" text="Number" />
      <Button mnemonicParsing="false" onAction="#changeSceneToDice" prefWidth="150.0" text="Dice" />
      <Button mnemonicParsing="false" onAction="#changeSceneToCard" prefWidth="150.0" text="Card" />
      <Button mnemonicParsing="false" onAction="#changeSceneToYesOrNo" prefWidth="150.0" text="Yes or No" />
      <Button mnemonicParsing="false" onAction="#changeSceneToCoin" prefWidth="150.0" text="Coin" />
      <Button mnemonicParsing="false" onAction="#changeSceneToRPS" prefWidth="150.0" text="Rock-Paper-Scissors" />
      <Button mnemonicParsing="false" onAction="#changeSceneToFood" prefWidth="150.0" text="Food" />
      <Button mnemonicParsing="false" onAction="#changeSceneToArrange" prefWidth="150.0" text="Arrange" />
      <Button mnemonicParsing="false" onAction="#changeSceneToPickUp" prefWidth="150.0" text="Pick Up" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</FlowPane>
