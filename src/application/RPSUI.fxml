<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane hgap="10.0" prefHeight="400.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RPSController">
   <children>
      <Button mnemonicParsing="false" onAction="#handleBack" text="&lt; Back" />
      <Label alignment="CENTER" prefHeight="17.0" prefWidth="455.0" text="Rock-Paper-Scissors" />
      <Button mnemonicParsing="false" onAction="#handleClear" text="Recent" />
      <TextField fx:id="num_tf" alignment="CENTER" promptText="No. of Player(s)">
         <FlowPane.margin>
            <Insets left="210.0" />
         </FlowPane.margin>
      </TextField>
      <Label fx:id="default_lb" text="Default is 1">
         <FlowPane.margin>
            <Insets left="250.0" />
         </FlowPane.margin>
      </Label>
      <FlowPane fx:id="list_fp" alignment="CENTER" prefHeight="170.0" prefWidth="578.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <FlowPane.margin>
            <Insets top="20.0" />
         </FlowPane.margin>
      </FlowPane>
      <Button mnemonicParsing="false" onAction="#handleRandom" text="Random">
         <FlowPane.margin>
            <Insets left="260.0" top="20.0" />
         </FlowPane.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</FlowPane>
