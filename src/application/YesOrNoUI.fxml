   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Button mnemonicParsing="false" onAction="#handleBack" text="&lt; Back" />
      <Label alignment="CENTER" prefHeight="17.0" prefWidth="455.0" text="YES or NO" />
      <Button mnemonicParsing="false" onAction="#handleRecent" text="Recent" />
      <Label prefHeight="14.0" prefWidth="575.0" />
      <HBox fx:id="list_hb" alignment="CENTER" prefHeight="182.0" prefWidth="576.0" />
      <Label prefHeight="14.0" prefWidth="575.0" />
      <Label prefHeight="17.0" prefWidth="258.0" />
      <Button mnemonicParsing="false" onAction="#handleRandom" text="Try" />
   </children>
</FlowPane>
