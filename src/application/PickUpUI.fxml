<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<FlowPane hgap="10.0" prefHeight="400.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PickUpController">
   <children>
      <Button mnemonicParsing="false" onAction="#handleBack" text="&lt; Back" />
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="453.0" text="Pick Up" />
      <Button mnemonicParsing="false" onAction="#handleClear" text="Recent" />
      <RadioButton fx:id="norepeat_rb" mnemonicParsing="false" text="No Repeat">
         <FlowPane.margin>
            <Insets left="250.0" />
         </FlowPane.margin>
      </RadioButton>
      <TextArea fx:id="input_ta" prefHeight="171.0" prefWidth="316.0">
         <FlowPane.margin>
            <Insets left="150.0" />
         </FlowPane.margin>
      </TextArea>
      <Label fx:id="randomed_lb" alignment="CENTER" prefHeight="54.0" prefWidth="99.0" text="?">
         <FlowPane.margin>
            <Insets left="250.0" />
         </FlowPane.margin>
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#handleRandom" text="Pick">
         <FlowPane.margin>
            <Insets left="275.0" />
         </FlowPane.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</FlowPane>
